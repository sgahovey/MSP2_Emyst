{
    "git.autorefresh": false,
    "git.enabled": true,
    "git.autoRepositoryDetection": false,
    "files.watcherExclude": {
        "**/var/**": true,
        "**/vendor/**": true,
        "**/node_modules/**": true,
        "**/.git/**": true
    },
    "files.exclude": {
        "**/var": true
    },
    "search.exclude": {
        "**/var": true,
        "**/vendor": true,
        "**/node_modules": true
    }
}


